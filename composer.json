    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "nunomaduro/termwind": "^2.3",
        "laravel/pint": "^1.20",
        "larastan/larastan": "^3.0",
        "rector/rector": "^2.0"
    },
    "autoload": {
